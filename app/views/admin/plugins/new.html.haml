.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", admin_plugins_path
      %li.active= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_plugin_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      Plugin
    .inner
      = form_for :plugin, :url => admin_plugins_path, :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
